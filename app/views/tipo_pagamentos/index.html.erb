<p id="notice"><%= notice %></p>

<h1>Listing Tipo Pagamentos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_pagamentos.each do |tipo_pagamento| %>
      <tr>
        <td><%= tipo_pagamento.tipo %></td>
        <td><%= link_to 'Show', tipo_pagamento %></td>
        <td><%= link_to 'Edit', edit_tipo_pagamento_path(tipo_pagamento) %></td>
        <td><%= link_to 'Destroy', tipo_pagamento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo pagamento', new_tipo_pagamento_path %>
